
<launch>
    <!-- Set up the path -->
    <arg name="yolo_env", default="($env YOLO_CONFIG)"/>
    <!-- Start the node and load param privately for this node -->
    <node pkg="yolo_detection" type="yolo_detection.py" name="yolo_detection" output="screen" />
        <!-- Load rosparam from the file -->
        <rosparam command="load" file="$(arg yolo_env)/"/>
    </node>
</launch>